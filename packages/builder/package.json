{
  "name": "vedix-builder",
  "version": "0.0.2",
  "description": "This is a json file that contains the basic information and content of the component",
  "main": "./dist/index.mjs",
  "scripts": {
    "build": "unbuild & pnpm gc",
    "dev": "unbuild --stub & pnpm gc",
    "prepublishOnly": "npm run build",
    "release": "npm publish",
    "start": "esno src/index.ts",
    "gc": "esno scripts/generate.ts dist",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "type": "./dist/index.d.ts"
    },
    "./components": "./dist/components.json"
  },
  "keywords": [],
